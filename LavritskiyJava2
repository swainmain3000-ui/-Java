package Lavritskiy;
import java.util.Scanner;

public class Lavritskiy2 {
    public static void main(String[] args){
        Scanner scanner = new Scanner(System.in);

        System.out.print("Введите первую строку (a): ");
        String a = scanner.nextLine();

        System.out.print("Введите вторую строку (b): ");
        String b = scanner.nextLine();

        if (a.equals(b)) {
            System.out.println("1. Строки идентичны (с учётом регистра)");
        } else {
            System.out.println("1. Строки неидентичны (с учётом регистра)");
        }

        //Если хотим не учитывать КАПС у букв, то вметсто equals используем equalsIgnoreCase

        scanner.close();
    }
}
